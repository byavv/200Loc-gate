  "license": "MIT",
  "scripts": {
    "build": "npm run clean:build && ./node_modules/.bin/gulp build --env production",
    "start": "cross-env NODE_ENV=production node .",
    "test": "./node_modules/.bin/gulp test:ci",
    "dev": "cross-env NODE_ENV=development DEBUG=proxy node .",    
    "pm2": "cross-env NODE_ENV=production pm2 start . --name=200loc -i max --no-daemon",
    "serve": "cross-env NODE_ENV=development ./node_modules/.bin/gulp",
    "clean": "npm run clean:coverage && npm run clean:build",
    "clean:coverage": "rm -rf ./coverage",
    "clean:build": "rm -rf ./explorer/build/*"
  },
